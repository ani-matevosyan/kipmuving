    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-babel": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "jquery": "^3.1.0",
    "laravel-mix": "^1.4.2",
    "lodash": "^4.17.4",
    "pump": "^1.0.2",
